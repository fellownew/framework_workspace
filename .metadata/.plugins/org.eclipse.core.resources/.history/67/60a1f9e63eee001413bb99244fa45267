<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="manager.member">
	<sql id="selecte-member">
		select member_id,member_pwd,member_name,member_email,member_date from member
	</sql>
	<resultMap type="member" id="member-resultmap">
		<id column="member_id" property="id"/>
		<result column="member_pwd" property="password"/>
		<result column="member_name" property="name"/>
		<result column="member_email" property="email"/>
		<result column="member_date" property="joinDate"/>
	</resultMap>
	
	<insert id="insertMember" parameterType="member">
		insert into member values (#{id},#{password},#{name},#{email},#{joinDate})
	</insert>
	
	<update id="updateMember" parameterType="member">
		update member set member_pwd=#{password},member_name=#{name},member_email=#{email},member_date=#{joinDate} where member_id=#{id}
	</update>
	
	<delete id="deleteMemberById" parameterType="string">
		
	</delete>
</mapper>
